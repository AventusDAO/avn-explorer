{
  "name": "@avn/search",
  "private": true,
  "packageManager": "yarn@3.3.0",
  "scripts": {
    "types:check": "tsc --pretty --noEmit",
    "build": "rm -rf lib && tsc",
    "build:watch": "tsc -w",
    "dev": "nodemon",
    "processor:start": "yarn db:migrate && node lib/index.js",
    "processor:debug": "node --inspect --enable-source-maps -r dotenv/config lib/index.js"
  },
  "dependencies": {
    "@avn/config": "^1.0.0",
    "@avn/types": "^1.0.0",
    "@avn/utils": "^1.0.0",
    "@subsquid/logger": "^0.3.0",
    "@subsquid/ss58": "^0.1.1",
    "@subsquid/substrate-processor": "^1.2.0",
    "@subsquid/typeorm-store": "^0.1.5",
    "class-validator": "^0.13.2",
    "dotenv": "^10.0.0",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@subsquid/cli": "^0.7.2",
    "@subsquid/substrate-typegen": "^1.5.0",
    "@types/node": "^18.11.9",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.3"
  }
}
