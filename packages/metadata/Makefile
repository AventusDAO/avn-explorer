# solochain

versions-solochain-dev:
	@yarn squid-substrate-metadata-explorer --chain wss://avn.uat.aventus.io --archive https://solo-archive-explorer.dev.aventus.io/graphql --out ./versions-solochain.dev.jsonl

versions-solochain-testnet:
	@yarn squid-substrate-metadata-explorer --chain wss://avn.test.aventus.io --archive https://solo-archive-explorer.public-testnet.aventus.io/graphql --out ./versions-solochain.testnet.jsonl

versions-solochain-mainnet:
	@yarn squid-substrate-metadata-explorer --chain wss://avn.aventus.io --archive https://solo-archive-explorer.mainnet.aventus.io/graphql --out ./versions-solochain.mainnet.jsonl

