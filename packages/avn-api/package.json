{
  "name": "@avn/avn-api",
  "private": true,
  "version": "1.0.0",
  "packageManager": "yarn@3.3.0",
  "main": "lib/index",
  "typings": "lib/index",
  "scripts": {
    "repl": "env NODE_ENV=debug node --experimental-repl-await --require ./scripts/repl.js",
    "types:check": "tsc --pretty --noEmit",
    "build": "rm -rf lib && tsc",
    "build:watch": "rm -rf lib && tsc -w"
  },
  "dependencies": {
    "@avn/config": "^1.0.0",
    "@polkadot/api": "^9.4.3",
    "@polkadot/types": "^9.4.3",
    "avn-types": "^1.2.2",
    "dotenv": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "typescript": "^4.9.3"
  },
  "resolutions": {
    "strip-ansi": "7.1.0",
    "color-convert": "2.0.1",
    "color-name": "1.1.4",
    "is-core-module": "2.13.1",
    "error-ex": "1.3.2",
    "has-ansi": "5.0.1"
  }
}
